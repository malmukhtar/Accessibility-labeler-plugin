{
    "name": "Accessibility Labeler",
    "description": "Accessibility classification tool. Classifies new or existing issues whether it's accessibility or Non-Accessibility issues. This tool classifies issues based on their titles (summaries) and descriptions using an NLP model based on Google Open-AI's pre-trained BERT model.",    "key": "predictool.app",
    "baseUrl": "https://accessibility-labeler-plugin.onrender.com/",
    "vendor": {
        "name": "Oakland University",
        "url": "https://oakland.edu/"
    },
    "authentication": {
        "type": "none"
    },
    "apiVersion": 1,
    "modules": {
        "jiraIssueContents": [
            {
                "icon": {
                    "width": 16,
                    "height": 16,
                    "url": "https://accessibility-labeler-plugin.onrender.com/app_icon"
                },
                "target": {
                    "type": "web_panel",
                    "url": "https://accessibility-labeler-plugin.onrender.com/Existing_IssueV3"
                },
                "tooltip": {
                    "value": "Check Label Accuracy"
                },
                "name": {
                    "value": "Label Accuracy"
                },
                "key": "classify-existing-issue",
                "conditions": [
                    {
                        "condition": "user_is_logged_in"
                    }
                ]
            }
        ]
    },
    "scopes": ["READ", "WRITE", "ADMIN"]
}

